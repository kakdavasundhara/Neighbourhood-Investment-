    "CLIENT_SECRET = '<Your Credential>' # your Foursquare Secret\n",
    "ACCESS_TOKEN = '<Your Credential>' #Your Foursquare Access Token\n",
    "VERSION = '20180605' # Foursquare API version"
   ]
  },
