     "execution_count": 10,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "# create map of Mumbai using latitude and longitude values\n",
    "map_mumbai = folium.Map(location=[latitude, longitude], zoom_start=9.5)\n",
    "\n",
    "# add markers to map\n",
    "for lat, lng, area, loca in zip(mumbai_df['Latitude'], mumbai_df['Longitude'], mumbai_df['Area'], mumbai_df['Location']):\n",
    "    label = '{}, {}'.format(area, loca)\n",
    "    label = folium.Popup(label, parse_html=True)\n",
    "    folium.CircleMarker(\n",
    "        [lat, lng],\n",
    "        radius=5,\n",
    "        popup=label,\n",
    "        color='blue',\n",
    "        fill=True,\n",
    "        fill_color='#3186cc',\n",
    "        fill_opacity=0.7).add_to(map_mumbai)  \n",
    "    \n",
    "map_mumbai"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [],
   "source": [
